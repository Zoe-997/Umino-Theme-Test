    .section-content {
      position: relative;
    }
    .section-content::before {
      display: table;
      content: "";
    }
    .section-content::after {
      display: table;
      content: "";
      clear: both;
    }
    .box-product-item {
      position: relative;
      border: solid 1px #000;
    }
    .box-product-item .head {
