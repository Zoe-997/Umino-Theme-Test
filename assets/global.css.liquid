    background: rgb(255 255 255 / 80%);
    opacity: 1;
  }
  .mfp-move-horizontal.mfp-removing .mfp-with-anim {
    transition: all 0.6s;
    opacity: 0;
    -webkit-transform: scale(0.9) translateY(50px);
    transform: scale(0.9) translateY(50px);
  }
  .mfp-move-horizontal.mfp-removing.mfp-bg {
    opacity: 0;
  }
