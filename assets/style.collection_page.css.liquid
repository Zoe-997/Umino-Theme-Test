  }
  .categories-menu-moible  .menu-init li{
  	position:relative;
    padding-left: 25px;
    line-height: 1;
  }
  .categories-menu-moible  .menu-init li span.collapse,
  .categories-menu-moible  .menu-init li span.expand{
    font-size: 0;
    position: absolute;
    right: -25px;
    width: 45px;
    height: 50px;
    line-height: 50px;
    text-align: center;
    border-radius: 3px;
    top: 0;
    cursor: pointer;
