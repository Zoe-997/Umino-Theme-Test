}
 .pside_opened #LoginModal.login_canvas {
	 -webkit-transform: none !important;
	 transform: none !important;
}
 #LoginModal.login_canvas {
	 position: fixed;
	 top: 0;
	 bottom: 0;
	 width: 300px;
	 background-color: #fff;
	 right: 0;
	 overflow-y: auto;
	 overflow-x: hidden;
	 -webkit-overflow-scrolling: touch;
	 transition: transform 0.2s;
	 -webkit-transform: translate3d(104%, 0, 0);
	 transform: translate3d(104%, 0, 0);
	 box-shadow: 0 0 10px 0 rgba(0, 0, 0, .5);
	 display: block;
	 z-index: 500;
}
#login_form .fixcl-scroll .fixcl-scroll-content {
	 padding: 20px;
}
  #login_form .fixcl-scroll .fixcl-scroll-content p label{
    font-size: 14px;
    color: #6b6b6b;
    font-weight: 500;
    line-height: 35px;
  }
  #login_form .fixcl-scroll .fixcl-scroll-content .input-group__field{
  	border: 1px solid #ccc;
    padding: 0 15px;
    border-radius: 3px;
    height: 45px;
    margin-bottom: 15px;
    line-height: 45px;
  }
  #login_form .fixcl-scroll .fixcl-scroll-content .button {
    border-radius: 3px;
    margin: 0;
    height: 45px;
    line-height: 45px;
    min-width: 195px;
  }
 #login_form {
	 position: absolute;
	 top: 0;
	 left: 0;
	 -webkit-transform: translateX(100%);
	 transform: translateX(100%);
	 width: 100%;
	 visibility: hidden;
	 pointer-events: none;
	 transition: transform 0.5s cubic-bezier(0.23, 1, 0.32, 1), visibility 0.5s cubic-bezier(0.23, 1, 0.32, 1), -webkit-transform 0.5s cubic-bezier(0.23, 1, 0.32, 1);
}
 .pside_opened #login_form {
	 -webkit-transform: translateX(0);
	 transform: translateX(0);
	 pointer-events: auto;
	 visibility: visible;
}
 a.push_side {
    text-decoration: none;
    height: 50px;
    display: inline-flex;
    align-items: center;
    justify-content: center;
    text-transform: uppercase;
    margin-bottom: 10px;
    font-size: 13px;
    font-weight: 500;
    width: 100%;
    border-radius: 3px;
    border: 1px solid var(--color-btn);
    padding: 0 15px;
    background-color: var(--color-btn);
    color: var(--color-btn-text);
       -webkit-transition: all 240ms linear 0ms;
    -khtml-transition: all 240ms linear 0ms;
    -moz-transition: all 240ms linear 0ms;
    -ms-transition: all 240ms linear 0ms;
    -o-transition: all 240ms linear 0ms;
    transition: all 240ms linear 0ms;
}
 a.push_side:hover {
    background: var(--color-btn-hover);
    border-color: var(--color-btn-hover);
    color: var(--color-btn-text_hover);
}
 button.button.button_primary {
    min-width: 150px;
    line-height: 50px;
    height: 50px;
    background-color: var(--color-btn);
    border-color: var(--color-btn);
    color: var(--color-btn-text);
    text-transform: uppercase;
    font-weight: 600;
    border-radius: 3px;
    font-size: 13px;
    padding: 0 30px;
    margin: 0 0 0 15px;
}
button.button.button_primary:hover {
	background: var(--color-btn-hover);
    color: var(--color-btn-text_hover);
}
 h2.password-content__title {
	 margin: 30px 0 20px;
	 font-size: 24px;
}
 p {
	 outline: 0;
}
