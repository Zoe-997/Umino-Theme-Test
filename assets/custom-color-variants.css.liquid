{%- if settings.custom_color_variants contains '|' -%}
{%- assign custom_color_variants = settings.custom_color_variants | split: '|' -%}
{%- else -%}
{%- assign custom_color_variants = settings.custom_color_variants | split: ',' -%}
{%- endif -%}
{%- for custom_color in custom_color_variants  -%}
      {%- assign color = custom_color | remove: ' ' | split: ':'  -%}
      .{{color[0]}},
      .standard-color-{{color[0]}}{
        background-color:{{color[1]}} !important;
      }
{% endfor %}
  
