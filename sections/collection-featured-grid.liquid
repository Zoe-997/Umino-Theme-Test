{% liquid
  assign sid = section.id 
  assign layout = section.settings.layout
  assign collection = section.settings.collection 
  assign se_blocks = section.blocks
  assign se_size = se_blocks.size
  assign style_layout =  section.settings.style_layout
  assign num_p_qty =  section.settings.num_p_qty
  
%}
  
{%- for i in (1..1) -%}  
<section id="laber_{{sid}}" class="cat_size_{{se_size}} pad-{{ section.settings.pad }}">
  <div class="{%if layout == 'full-width' %}{{layout}}{% else %}container{%endif%}" >   
    {% if section.settings.title != blank or section.settings.sub_title!= blank %}
		<div class="wrap_title {{ section.settings.text_size }}">
		  {% if section.settings.title != blank %}
		  <h3 class="section-title">{{section.settings.title}}</h3>  
		  {% endif %}
          {% if section.settings.sub_title!= blank %}
		  <span class="section-subtitle">{{section.settings.sub_title}}</span>
		  {% endif %}
		</div>
    {% endif %}
    <div class="row">
      
		{%- case se_size -%}
			{%- when 1 -%}
				{%- render 'cat-grid-item',block:se_blocks[0],grid:grid,num_p_qty:num_p_qty, cimg:'https://placehold.jp/100/edf6f5/fff/1880x820.png?text=1880x820',ratio_demo:'43.617' ,ccol:'col-12 pad',for_i:1 -%}
			{%- when 2 -%}	
				{%- render 'cat-grid-item',block:se_blocks[0],grid:grid,num_p_qty:num_p_qty, cimg:'https://placehold.jp/60/edf6f5/fff/930x820.png?text=930x820',ratio_demo:'88.172',ccol:'col-md-6 col-12 pad',for_i:1 -%}
				{%- render 'cat-grid-item',block:se_blocks[1],grid:grid,num_p_qty:num_p_qty, cimg:'https://placehold.jp/60/f6e8e3/fff/930x820.png?text=930x820',ratio_demo:'88.172',ccol:'col-md-6 col-12 pad',for_i:2 -%}
			{%- when 3 -%}	
				{%- render 'cat-grid-item',block:se_blocks[0],grid:grid,num_p_qty:num_p_qty, cimg:'https://placehold.jp/30/edf6f5/fff/613x400.png?text=613x400',ratio_demo:'65.252',ccol:'col-md-4 col-12 pad',for_i:1 -%}
				{%- render 'cat-grid-item',block:se_blocks[1],grid:grid,num_p_qty:num_p_qty, cimg:'https://placehold.jp/30/f6e8e3/fff/613x400.png?text=613x400',ratio_demo:'65.252',ccol:'col-md-4 col-12 pad',for_i:2 -%}
				{%- render 'cat-grid-item',block:se_blocks[2],grid:grid,num_p_qty:num_p_qty, cimg:'https://placehold.jp/30/dae9e4/fff/613x400.png?text=613x400',ratio_demo:'65.252',ccol:'col-md-4 col-12 pad',for_i:3 -%}
		{%- when 4 -%}	

				{%- render 'cat-grid-item',block:se_blocks[0],grid:grid,num_p_qty:num_p_qty, cimg:'https://placehold.jp/30/56cfe1/fff/930x400.png?text=930x400',ratio_demo:'43.010',ccol:'col-md-6 col-12 pad',for_i:1 -%}
				{%- render 'cat-grid-item',block:se_blocks[1],grid:grid,num_p_qty:num_p_qty, cimg:'https://placehold.jp/30/f6e8e3/fff/930x400.png?text=930x400',ratio_demo:'43.010',ccol:'col-md-6 col-12 pad',for_i:2 -%}
				{%- render 'cat-grid-item',block:se_blocks[2],grid:grid,num_p_qty:num_p_qty, cimg:'https://placehold.jp/30/dae9e4/fff/930x400.png?text=930x400',ratio_demo:'43.010',ccol:'col-md-6 col-12 pad',for_i:3 -%}
				{%- render 'cat-grid-item',block:se_blocks[3],grid:grid,num_p_qty:num_p_qty, cimg:'https://placehold.jp/30/f0efe6/fff/930x400.png?text=930x400',ratio_demo:'43.010',ccol:'col-md-6 col-12 pad',for_i:4 -%}

		{%- when 5 -%}	
                  <div class="col-md-6 col-12 pad">
                    {%- render 'cat-grid-item',block:se_blocks[0],grid:grid,num_p_qty:num_p_qty, cimg:'https://placehold.jp/30/edf6f5/fff/690x690.png?text=690x690',ratio_demo:'100',ccol:'',for_i:1 -%}
                  </div>
                  <div class="col-md-6 col-12 pad"><div class="row">
                    {%- render 'cat-grid-item',block:se_blocks[1],grid:grid,num_p_qty:num_p_qty, cimg:'https://placehold.jp/30/f6e8e3/fff/330x330.png?text=330x330',ratio_demo:'100',ccol:'col-md-6 col-12 pad',for_i:2 -%}
                    {%- render 'cat-grid-item',block:se_blocks[2],grid:grid,num_p_qty:num_p_qty, cimg:'https://placehold.jp/30/dae9e4/fff/330x330.png?text=330x330',ratio_demo:'100',ccol:'col-md-6 col-12 pad',for_i:3 -%}
