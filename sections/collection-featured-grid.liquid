                {%- render 'cat-grid-item',block:se_blocks[5],grid:grid,num_p_qty:num_p_qty, cimg:'https://placehold.jp/30/f1e8eb/222/405x820.png?text=405x820',ratio_demo:'202.469',ccol:'col-md-6 col-12 pad',for_i:6 -%}
				</div></div>
		{%- when 7 -%}
				<div class="col-md-6 col-12 padding-0-10"><div class="row">
				{%- render 'cat-grid-item',block:se_blocks[0],grid:grid,num_p_qty:num_p_qty, cimg:'https://placehold.jp/30/a8dacc/fff/337x217.png?text=337x217',ratio_demo:'64.391',ccol:'col-md-6 col-12',for_i:1 -%}
				{%- render 'cat-grid-item',block:se_blocks[1],grid:grid,num_p_qty:num_p_qty, cimg:'https://placehold.jp/30/a8dacc/fff/337x217.png?text=337x217',ratio_demo:'64.391',ccol:'col-md-6 col-12',for_i:2 -%}
				{%- render 'cat-grid-item',block:se_blocks[2],grid:grid,num_p_qty:num_p_qty, cimg:'https://placehold.jp/30/a8dacc/fff/695x447.png?text=695x447',ratio_demo:'67.83',ccol:'col-12',for_i:3 -%}
				</div></div>
				<div class="col-md-6 col-12 padding-0-10"><div class="row">
				{%- render 'cat-grid-item',block:se_blocks[3],grid:grid,num_p_qty:num_p_qty, cimg:'https://placehold.jp/30/a8dacc/fff/695x447.png?text=695x447',ratio_demo:'67.83',ccol:'col-12',for_i:4 -%}
				{%- render 'cat-grid-item',block:se_blocks[4],grid:grid,num_p_qty:num_p_qty, cimg:'https://placehold.jp/30/a8dacc/fff/337x217.png?text=337x217',ratio_demo:'64.391',ccol:'col-md-6 col-12',for_i:5 -%}
				{%- render 'cat-grid-item',block:se_blocks[5],grid:grid,num_p_qty:num_p_qty, cimg:'https://placehold.jp/30/a8dacc/fff/337x217.png?text=337x217',ratio_demo:'64.391',ccol:'col-md-6 col-12',for_i:6 -%}
				</div></div>
      			{%- render 'cat-grid-item',block:se_blocks[6],grid:grid,num_p_qty:num_p_qty, cimg:'https://placehold.jp/30/a8dacc/fff/1410x447.png?text=1410x447',ratio_demo:'31.702',ccol:'col-12',for_i:7 -%}
		{%- when 8 -%}
				{%- render 'cat-grid-item',block:se_blocks[0],grid:grid,num_p_qty:num_p_qty, cimg:'https://placehold.jp/30/a8dacc/fff/400x400.png?text=400x400',ratio_demo:'100',ccol:'col-md-3 col-12',for_i:1 -%}
				{%- render 'cat-grid-item',block:se_blocks[1],grid:grid,num_p_qty:num_p_qty, cimg:'https://placehold.jp/30/a8dacc/fff/400x400.png?text=400x400',ratio_demo:'100',ccol:'col-md-3 col-12',for_i:2 -%}
				{%- render 'cat-grid-item',block:se_blocks[2],grid:grid,num_p_qty:num_p_qty, cimg:'https://placehold.jp/30/a8dacc/fff/400x400.png?text=400x400',ratio_demo:'100',ccol:'col-md-3 col-12',for_i:3 -%}
				{%- render 'cat-grid-item',block:se_blocks[3],grid:grid,num_p_qty:num_p_qty, cimg:'https://placehold.jp/30/a8dacc/fff/400x400.png?text=400x400',ratio_demo:'100',ccol:'col-md-3 col-12',for_i:4 -%}
				{%- render 'cat-grid-item',block:se_blocks[4],grid:grid,num_p_qty:num_p_qty, cimg:'https://placehold.jp/30/a8dacc/fff/400x400.png?text=400x400',ratio_demo:'100',ccol:'col-md-3 col-12',for_i:5 -%}
				{%- render 'cat-grid-item',block:se_blocks[5],grid:grid,num_p_qty:num_p_qty, cimg:'https://placehold.jp/30/a8dacc/fff/400x400.png?text=400x400',ratio_demo:'100',ccol:'col-md-3 col-12',for_i:6 -%}
				{%- render 'cat-grid-item',block:se_blocks[6],grid:grid,num_p_qty:num_p_qty, cimg:'https://placehold.jp/30/a8dacc/fff/400x400.png?text=400x400',ratio_demo:'100',ccol:'col-md-3 col-12',for_i:7 -%}
