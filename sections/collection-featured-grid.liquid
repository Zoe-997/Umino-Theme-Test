                    {%- render 'cat-grid-item',block:se_blocks[4],grid:grid,num_p_qty:num_p_qty, cimg:'https://placehold.jp/30/efefef/fff/330x330.png?text=330x330',ratio_demo:'100',ccol:'col-md-6 col-12 pad',for_i:4 -%}
                  </div>
                  </div>
		{%- when 6 -%}
				<div class="col-md-6 col-12 pad"><div class="row">
				{%- render 'cat-grid-item',block:se_blocks[0],grid:grid,num_p_qty:num_p_qty, cimg:'https://placehold.jp/30/edf6f5/222/820x405.png?text=820x405',ratio_demo:'49.39',ccol:'col-md-12 col-12 pad',for_i:1 -%}
				{%- render 'cat-grid-item',block:se_blocks[1],grid:grid,num_p_qty:num_p_qty, cimg:'https://placehold.jp/30/f6e8e3/222/405x405.png?text=405x405',ratio_demo:'100',ccol:'col-md-6 col-12 pad',for_i:2 -%}
				{%- render 'cat-grid-item',block:se_blocks[2],grid:grid,num_p_qty:num_p_qty, cimg:'https://placehold.jp/30/dae9e4/222/405x405.png?text=405x405',ratio_demo:'100',ccol:'col-md-6 col-12 pad',for_i:3 -%}
                
				</div></div>
				<div class="col-md-6 col-12 pad"><div class="row">
                <div class="col-md-6 col-12 pad">  
                {%- render 'cat-grid-item',block:se_blocks[3],grid:grid,num_p_qty:num_p_qty, cimg:'https://placehold.jp/30/f0efe6/222/405x405.png?text=405x405',ratio_demo:'100',for_i:4 -%}  
				{%- render 'cat-grid-item',block:se_blocks[4],grid:grid,num_p_qty:num_p_qty, cimg:'https://placehold.jp/30/efefef/222/405x405.png?text=405x405',ratio_demo:'100',for_i:5 -%}
